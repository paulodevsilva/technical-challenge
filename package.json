    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
