    "json2csv": "^5.0.7",
    "module-alias": "^2.2.2",
    "mongoose": "^6.3.3",
    "multer": "^1.4.4",
    "node-cron": "^3.0.0",
    "node-schedule": "^2.1.0",
    "winston": "^3.7.2",
    "xml2json": "^0.12.0"
