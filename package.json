    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.33",
    "@types/node-cron": "^3.0.1",
    "@types/node-schedule": "^2.1.0",
    "@types/winston": "^2.4.4",
    "@types/xml2json": "^0.11.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv": "^16.0.1",
